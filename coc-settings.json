{
  "go.goplsArgs": [
    "-remote=auto",
    "-logfile=auto",
    "-debug=\"\"",
    "-rpc.trace"
  ],
  "go.goplsPath": "~/code/gopath/bin/gopls",
  "go.goplsOptions": {
    "usePlaceholders": false,
    "linksInHover": false,
    "diagnosticsDelay": "500ms",
    "staticcheck": true,
    "importShortcut": "Both"
  },
  "languageserver": {
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "snippet.choicesMenuPicker": false,
  "suggest.languageSourcePriority": 99,
  "coc.source.tag.priority": 30,
  "coc.source.buffer.enable": true,
  "coc.source.buffer.shortcut": "B",
  "coc.source.around.enable": true,
  "coc.source.around.priority": 20,
  "coc.source.file.enable": true,
  "coc.source.file.shortcut": "F",
  "coc.source.file.priority": 50,
  "coc.source.around.shortcut": "A",
  "coc.source.around.disableSyntaxes": [],
  "coc.source.buffer.priority": 60,
  "coc.source.file.disableSyntaxes": [],
  "coc.source.buffer.ignoreGitignore": true,
  "coc.source.buffer.disableSyntaxes": [],
  "coc.source.file.triggerCharacters": ["/", "\\"],
  "coc.source.file.trimSameExts": [".ts", ".js", ".lua"],
  "coc.source.file.ignoreHidden": true,
  "coc.source.file.ignorePatterns": [],
  "tree.key.toggle": "t",
  "tree.key.toggleSelection": "<space>",
  "tree.key.actions": "<tab>",
  "tree.key.collapseAll": "M",
  "tree.key.invoke": "<cr>",
  "tree.key.close": "<esc>",
  "tree.key.activeFilter": "f",
  "tree.key.selectNext": "<C-j>",
  "tree.key.selectPrevious": "<C-k>",
  "Lua.completion.callSnippet": "Replace",
  "Lua.completion.keywordSnippet": "Replace",
  "Lua.signatureHelp.enable": true,
  "Lua.telemetry.enable": true,
  "Lua.completion.autoRequire": true,
  "Lua.completion.displayContext": 1,
  "Lua.hint.enable": false,
  "list.maxPreviewHeight": 9999,
  "list.alignColumns": true,
  "list.extendedSearchMode": true,
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
  "sumneko-lua.prompt": true,
  "sumneko-lua.inlayHints.typeHintsPrefix": "« ",
  "inlayHint.enable": true,
  "inlayHint.enableParameter": true,
  "inlayHint.refreshOnInsertMode": false,
  "suggest.noselect": false,
  "documentHighlight.timeout": 500,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.autoTrigger": "always",
  "suggest.defaultSortMethod": "length",
  "suggest.detailField": "preview",
  "suggest.formatItems": ["kind", "abbr", "menu", "shortcut"],
  "suggest.enablePreselect": true,
  "suggest.fixInsertedWord": true,
  "suggest.floatConfig": {
    "border": false,
    "rounded": false,
    "borderhighlight": "CocFloating",
    "maxWidth": 100,
    "winblend": 5,
    "shadow": false
  },
  "suggest.pumFloatConfig": {
    "border": false,
    "rounded": false,
    "borderhighlight": "CocFloating",
    "winblend": 10,
    "shadow": false
  },
  "suggest.localityBonus": false,
  "suggest.maxCompleteItemCount": 50,
  "suggest.minTriggerInputLength": 2,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.removeDuplicateItems": true,
  "suggest.selection": "recentlyUsed",
  "suggest.snippetIndicator": "~",
  "suggest.snippetsSupport": true,
  "suggest.timeout": 5000,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.triggerCompletionWait": 50,
  "suggest.reversePumAboveCursor": false,
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "f",
    "function": "f",
    "constructor": "f",
    "field": "m",
    "variable": "v",
    "class": "C",
    "interface": "I",
    "module": "M",
    "property": "m",
    "unit": "U",
    "value": "v",
    "enum": "E",
    "keyword": "k",
    "snippet": "SN",
    "color": "v",
    "file": "F",
    "reference": "r",
    "folder": "F",
    "enumMember": "m",
    "constant": "v",
    "struct": "S",
    "event": "E",
    "operator": "O",
    "typeParameter": "T",
    "default": ""
  },
  "signature.enable": true,
  "signature.preferShownAbove": true,
  "signature.triggerSignatureWait": 500,
  "signature.floatConfig": {
    "border": false,
    "rounded": false,
    "borderhighlight": "CocFloating",
    "maxWidth": 100,
    "winblend": 15,
    "maxHeight": 30,
    "shadow": false
  },
  "signature.target": "float",
  "signature.hideOnTextChange": false,
  "colors.filetypes": [
    "css",
    "go",
    "html",
    "javascript",
    "lua",
    "typescript",
    "vim",
    "vue"
  ],
  "snippet.statusText": "SNIP",
  "snippet.highlight": false,
  "snippet.nextPlaceholderOnDelete": true,
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.formatOnSaveFiletypes": [],
  "coc.preferences.enableMarkdown": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.messageLevel": "warning",
  "coc.preferences.formatOnType": false,
  "json.format.enable": true,
  "json.enable": true,
  "hover.autoHide": true,
  "hover.target": "float",
  "hover.previewMaxHeight": 12,
  "hover.floatConfig": {
    "maxHeight": 30,
    "border": false,
    "shadow": false,
    "borderhighlight": "CocFloating",
    "rounded": false,
    "maxWidth": 100,
    "winblend": 10
  },
  "notification.border": true,
  "notification.focusable": true,
  "notification.timeout": 2000,
  "notification.winblend": 30,
  "notification.statusLineProgress": true,
  "diagnostic.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.autoRefresh": true,
  "diagnostic.messageDelay": 500,
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextLevel": "information",
  "diagnostic.enableMessage": "always",
  "diagnostic.level": "information",
  "diagnostic.highlightLimit": 300,
  "diagnostic.enableSign": true,
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.showDeprecated": true,
  "diagnostic.warningSign": "ﴞ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯧ",
  "diagnostic.errorSign": "",
  "diagnostic.messageTarget": "float",
  "diagnostic.format": "%message\n[%source:%code]",
  "cSpell.numSuggestions": 4,
  "cSpell.showStatus": true,
  "cSpell.language": "en",
  "cSpell.checkLimit": 1024,
  "cSpell.maxNumberOfProblems": 100,
  "cSpell.minWordLength": 4,
  "cSpell.maxDuplicateProblems": 3,
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Information",
