{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        "vendor/"
      ],
      "args": ["-remote=unix;/tmp/gopls-daemon-socket", "-logfile=auto", "-debug=\"\"", "-rpc.trace"],
      "filetypes": [
        "go"
      ]
    },
    "clangd": {
      "command": "clangd-12",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        "build/compile_commands.json"
      ],
      "filetypes": [
        "c",
        "cc",
        "cpp",
        "c++",
        "objc",
        "objcpp"
      ]
    }
  },
  "suggest.enablePreselect": true,
  "suggest.noselect": true,
  "suggest.maxCompleteItemCount": 20,
  "suggest.timeout": 10000,
  "suggest.enablePreview": true,
  "suggest.autoTrigger": "always",
  "suggest.floatEnable": true,
  "suggest.localityBonus": true,
  "suggest.minTriggerInputLength": 2,

  "coc.preferences.jumpCommand": "edit",

  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.autoRefresh": true,
  "diagnostic.messageDelay": 200,
  "diagnostic.virtualText": false,
  "diagnostic.enableMessage": "always",
  "cSpell.userWords": [
    "Ecom",
    "Exts",
    "Ptcode",
    "Ptcodes",
    "Succ",
    "TEMAI",
    "aweme",
    "basep",
    "byted",
    "caijing",
    "callbackdto",
    "commondto",
    "conv",
    "ctxvalues",
    "ectrade",
    "ginex",
    "gjson",
    "gocarina",
    "gocsv",
    "gonic",
    "kitc",
    "kitex",
    "kitutil",
    "ktest",
    "logid",
    "npwd",
    "payapi",
    "paywaydto",
    "sjson",
    "tidwall",
    "toutiao",
    "xiaodian"
  ]
}
